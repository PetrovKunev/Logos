## SMTP settings used by app/api/contact/route.ts
# Host: mail.privateemail.com
SMTP_HOST=mail.privateemail.com

# 993 is IMAP (incoming). For SMTP use 465 (SSL) or 587 (STARTTLS).
SMTP_PORT=465
SMTP_SECURE=true

# Your mailbox login (often the full email address)
SMTP_USER=office@academylogos.com
SMTP_PASS=CHANGE_ME

## Where contact form emails should be delivered
CONTACT_TO=office@academylogos.com

# Optional; defaults to SMTP_USER if omitted.
# Some providers require FROM to match the authenticated mailbox.
CONTACT_FROM=office@academylogos.com

## Upstash Redis (rate limiting)
UPSTASH_REDIS_REST_URL=CHANGE_ME
UPSTASH_REDIS_REST_TOKEN=CHANGE_ME


